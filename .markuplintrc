{
  "specs": {
    "\\.[jt]sx?$": "@markuplint/react-spec"
  },
  "parser": {
    "\\.[jt]sx?$": "@markuplint/jsx-parser"
  },
  "extends": [
    "markuplint:recommended"
  ],
  "nodeRules": [
    {
      "selector": "html",
      "rules": {
        "invalid-attr": {
          "options": {
            "allowAttrs": ["prefix"]
          }
        }
      }
    }
  ]
}
